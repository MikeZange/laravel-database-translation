{
    "name": "mike-zange/laravel-database-translation",
    "description": "A Laravel package to load translation strings from database with fallback to file system",
    "keywords": ["laravel", "translation", "language", "database"],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.5",
        "spatie/laravel-translatable": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "MikeZange\\LaravelDatabaseTranslation\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Zange",
            "email": "mike.zange.1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "MikeZange\\LaravelDatabaseTranslation\\TranslationServiceProvider"
            ]
        }
    }
}
